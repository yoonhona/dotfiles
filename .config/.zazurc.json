{
  "hotkey": "cmd+alt+space",
  "theme": "tinytacoteam/zazu-light-theme",
  "displayOn": "detect",
  "plugins": [
    "tinytacoteam/zazu-calculator",
    {
      "name": "tinytacoteam/zazu-file-finder",
      "variables": {
        "append": true,
        "directories": {
          "filePath": [
            "~"
          ],
          "excludeName": [
            "node_modules"
          ],
          "excludePath": [
            "~/Library"
          ]
        }
      }
    },
    "tinytacoteam/zazu-template",
    "tinytacoteam/zazu-system",
    "tinytacoteam/zazu-package-manager",
    {
      "name": "tinytacoteam/zazu-fallback",
      "variables": {
        "rootSearches": [
          "google"
        ],
        "prefixSearches": {
          "endic": {
            "icon": "fa-book",
            "name": "네이버 영어 사전",
            "url": "https://endic.naver.com/search.nhn?query="
          },
          "ciu": {
            "icon": "fa-check",
            "name": "Can I use",
            "url": "https://caniuse.com/#search="
          },
          "sof": {
            "icon": "fa-check",
            "name": "stackoverflow",
            "url": "https://stackoverflow.com/search?q="
          },
          "dh": {
            "icon": "fa-check",
            "name": "docker hub",
            "url": "https://hub.docker.com/search?type=image&q="
          },
          "mr": {
            "icon": "fa-book",
            "name": "미스터 블루 작품 검색",
            "url": "https://www.mrblue.com/search?keyword="
          }
        }
      }
    },
    "tinytacoteam/zazu-chrome-bookmarks",
    "danielbayerlein/zazu-devdocs"
  ]
}
